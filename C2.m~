
Npts=10;
h=.5;

a=exp(1i*9*pi/10);
b=exp(1i*pi/10);
X=-10:h:10;
Y=X;

A31 = @(x,y) A(x,y,a,b,Npts);

xCount=0;
for x = X
    xCount = xCount + 1;
    yCount = 0;
    for y = Y
        yCount = yCount + 1;
        u(xCount,yCount) = A31(x,y);
        fprintf('%.0f %%', 10*Count/length(X) + yCount);
    end
end

surf(X,Y,abs(u));